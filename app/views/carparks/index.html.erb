<p id="notice"><%= notice %></p>

<h1>Carparks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carparks.each do |carpark| %>
      <tr>
        <td><%= carpark.name %></td>
        <td><%= carpark.address %></td>
        <td><%= link_to 'Show', carpark %></td>
        <td><%= link_to 'Edit', edit_carpark_path(carpark) %></td>
        <td><%= link_to 'Destroy', carpark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Carpark', new_carpark_path %>
